{
  "name": "startz/oauth2-etsy",
  "license": "MIT",
  "description": "Etsy OAuth2 Client Provider for The PHP League OAuth2-Client",
  "authors":[
    {
      "name": "Chuck Burgess",
      "email": "cdburgess@gmail.com"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "etsy"
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "league/oauth2-client": "^2.6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.6.6",
    "mockery/mockery": "~1.4",
    "squizlabs/php_codesniffer": "~3.5",
    "phpmd/phpmd": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "Startz\\OAuth2\\Client\\Provider\\": "src/Provider/",
      "Startz\\OAuth2\\Client\\Provider\\Exception\\": "src/Provider/Exception/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StartZ\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  }
}